{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
      "no-plusplus": "off",
      "comma-dangle": ["error", "never"],
      "no-param-reassign": [2, { "props": false }],
      "class-methods-use-this": "off",
      "consistent-return": "off",
      "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true } }]    }
}